    "/Weather/Get3hData": {
      "post": {
        "tags": [
          "weather"
        ],
        "summary": "Get 3hours weather data",
        "description": "input your locate",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "longitude": {
                    "type": "string",
                    "example": 121.662487566784
                  },
                  "latitude": {
                    "type": "string",
                    "example": 25.06715187342581
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "成功回傳消息",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string"
                }
              }
            }
          },
          "400": {
            "description": "Invalid location supplied"
          },
          "404": {
            "description": "Location not found"
          }
        }
      }
    },
